<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.com.ultrawise.bank.bnk.model.BnkBank" table="bnk_bank"
		catalog="bank">
		<id name="bnkBankId" type="java.lang.String">
			<column name="BNK_BANK_ID" length="255" />
			<generator class="assigned" />
		</id>
		<property name="bnkCode" type="java.lang.String">
			<column name="BNK_CODE" length="4" />
		</property>
		<property name="bnkBankEname" type="java.lang.String">
			<column name="BNK_BANK_ENAME" length="60" />
		</property>
		<property name="bnkBankCname" type="java.lang.String">
			<column name="BNK_BANK_CNAME" length="60" />
		</property>
	</class>
</hibernate-mapping>

